#!name=🌈策略面板
#!desc=📍集合～

[Panel]
策略面板 = script-name=Surge启动时长,update-interval=1
策略面板 = script-name=节假日,update-interval=28800
策略面板 = script-name=流媒体解锁检测,update-interval=7200
策略面板 = script-name=机场流量信息,update-interval=7200
策略面板 = script-name=当前网络信息,update-interval=5

[Script]
Surge启动时长 = type=generic,timeout=30,script-path= https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Surge-Pro/Moore/SurgePro.js,script-update-interval=0 ,argument=icon=paperplane.circle&color=#f6c970
节假日 = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/smartmimi/conf/master/surge/timecard.js,script-update-interval=0
流媒体解锁检测 = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/githubdulong/Script/master/Stream-All.js,script-update-interval=0,argument=title=流媒体解锁检测&icon=play.tv.fill&color=#FF2D55
机场流量信息 = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Sub-info/Moore/Sub-info.js,script-update-interval=0,argument=url=机场订阅&reset_day=15&title=Nexitally&icon=n.square=#007aff
当前网络信息 = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Network-Info/Moore/Network-Info.js,script-update-interval=0

[Host]
ip-api.com = 208.95.112.1
