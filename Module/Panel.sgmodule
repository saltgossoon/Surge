#!name=面板🥳
#!desc=网络详情、流媒体解锁状态等～🥳

[Panel]
#Surge Pro启动时间 手动刷新可配置重载
SurgePro_ReloadProfile = script-name=SurgePro_ReloadProfile,update-interval=10

#节假日时间
Timecard = script-name=Timecard,update-interval=3600

#快速清除Surge的DNS缓存
#FlushDNS = script-name=FlushDNS,update-interval=600

#机场流量信息
Sub_info = script-name=Sub_info,update-interval=3600

#流媒体解锁状态
stream-all = script-name=stream-all, title="流媒体解锁检测", content="请刷新面板", update-interval=3600

#网络详情
NET_info = script-name=NET_info,update-interval=10

[Script]
#Surge Pro启动时间 手动刷新可配置重载
SurgePro_ReloadProfile = type=generic,timeout=30,script-path= https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Surge-Pro/Moore/SurgePro.js ,argument=icon=paperplane.circle&color=#f6c970

#节假日时间
Timecard = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/smartmimi/conf/master/surge/timecard.js

#快速清除Surge的DNS缓存
#FlushDNS = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Flush-DNS/Moore/Flush-DNS.js,argument=title=DNS FLush&icon=cube&color=#afe59a&server=false

#机场流量信息
Sub_info = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Sub-info/Moore/Sub-info.js,script-update-interval=0,argument=url=转换后的机场订阅&title=Nexitally&icon=n.square=#007aff

#流媒体解锁状态
stream-all = type=generic, timeout=30, script-path=https://raw.githubusercontent.com/LucaLin233/Luca_Conf/main/Surge/JS/stream-all.js

#网络详情
NET_info = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/saltgossoon/Surge/main/PanelScripts/net_info.js,argument=icon=externaldrive.connected.to.line.below&color=#9a7ff7&group=🥤 奶昔
